function generate(){var e,n=document.getElementById("download"),t=document.getElementById("btn"),a=document.getElementById("download").href,l=0,d=document.createElement("span");n.parentNode.replaceChild(d,n),e=setInterval(function(){--l<0?(d.parentNode.replaceChild(n,d),clearInterval(e),window.location.replace(a),n.style.display="inline"):(d.innerHTML="<i class='fa fa-clock-o' aria-hidden='true'/> File siap diunduh dalam "+l.toString()+" Detik....",t.style.display="none")},1e3)}$(function(){if($("#Sid-kanan").length){var o=$("#Sid-kanan"),t=$("#Sid-kanan").offset().top,i=$("#Sid-kanan").height();$(window).scroll(function(){var s=$(".bawah-iklan").offset().top-i-20,f=$(window).scrollTop();if(f>t?o.css({position:"fixed",top:5,right:0}):o.css("position","static"),f>s){var n=s-f;o.css({top:n})}})}});$(function(){if($("#sidebar1").length){var o=$("#sidebar1"),t=$("#sidebar1").offset().top,i=$("#sidebar1").height();$(window).scroll(function(){var s=$(".post-footer").offset().top-i-20,f=$(window).scrollTop();if(f>t?o.css({position:"fixed",top:0,width:"268px"}):o.css("position","static"),f>s){var n=s-f;o.css({top:n})}})}});
